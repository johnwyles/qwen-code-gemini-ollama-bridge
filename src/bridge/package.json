{
  "name": "gemini-openai-bridge",
  "version": "1.0.0",
  "description": "Translation layer between qwen-code and OpenAI-compatible APIs",
  "main": "bridge.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest integration.test.js",
    "start": "node bridge.js"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "jest": "^30.0.5",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "bridge.js",
      "!node_modules/**"
    ],
    "coverageReporters": ["text", "lcov", "html"]
  },
