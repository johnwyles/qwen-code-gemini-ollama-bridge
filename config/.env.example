# Qwen Code Docker Configuration
# Copy this file to ../.env and customize with your settings

# API Configuration
OPENAI_BASE_URL=http://localhost:11434/v1
OPENAI_API_KEY=your-api-key-here
OPENAI_MODEL=qwen3-coder:latest

# Authentication Type
GEMINI_DEFAULT_AUTH_TYPE=openai

# Gemini-OpenAI Bridge Configuration
# Set to true to enable the bridge for API request routing
USE_GEMINI_BRIDGE=false

# Bridge target URL (where to forward requests)
# Defaults to OPENAI_BASE_URL if not specified
BRIDGE_TARGET_URL=http://your-target-server:port/v1

# Bridge listening port
BRIDGE_PORT=8080

# Enable debug logging for bridge
BRIDGE_DEBUG=false